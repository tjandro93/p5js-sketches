{
  "name": "ng-p5js-sketches",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --write .",
    "prettier-and-lint": "npm run prettier && npm run lint",
    "build-schematics": "cd sketch-schematics && npm install && npm run build",
    "build-schematics-watch": "cd sketch-schematics && npm install && npm run build-watch",
    "generate-sketch": "schematics ./sketch-schematics/src/collection.json:sketch --debug=false",
    "generate-routes": "schematics ./sketch-schematics/src/collection.json:sketch-routes --debug=false"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics-cli": "^13.3.7",
    "@angular/animations": "~13.3.11",
    "@angular/cdk": "^13.3.8",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/material": "^13.3.8",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "p5": "^1.4.1",
    "p5.js-svg": "^1.3.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.7",
    "@angular/cli": "~13.3.7",
    "@angular/compiler-cli": "~13.3.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/p5": "^1.4.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "2.6.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-angular": "^3.0.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.6.4"
  }
}
