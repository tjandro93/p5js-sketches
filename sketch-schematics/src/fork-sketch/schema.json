{
  "$schema": "http://json-schema.org/schema",
  "$id": "ForkSketch",
  "title": "Fork Sketch",
  "type": "object",
  "properties": {
    "source": {
      "type": "string",
      "format": "path",
      "description": "The sketch to fork"
    },
    "target": {
      "type": "string",
      "format": "path",
      "description": "The sketch to create"
    }
  },
  "required": ["source", "target"]
}
