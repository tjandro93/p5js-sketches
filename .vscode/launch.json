    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run sketch schematic",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/@angular-devkit/schematics-cli/bin/schematics.js",
            "args": [
                "./sketch-schematics/src/collection.json:sketch",
                "--name=foo"
            ],
            "outputCapture": "std"
        }
    ]
}